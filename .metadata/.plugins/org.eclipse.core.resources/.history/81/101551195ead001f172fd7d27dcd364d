package Threads;
import java.util.*;

class num extends Thread{
	@Override
	public void run() {
		// TODO Auto-generated method stub
			
			try {
				for(int i=0;i<5;i++) {
					System.out.println(i);

				}
				Thread.sleep(2000);

		}catch (Exception e) {
			// TODO: handle exception
		}
	}
}

class charec implements Runnable{
	@Override
	public void run() {
		// TODO Auto-generated method stub
		
		try {
			for(char i='A';i<'E';i++){
				System.out.println(i);

			}
			Thread.sleep(2000);

		}catch (Exception e) {
			// TODO: handle exception
		}
	}
}
public class threads {
	public static void main(String[] args) {
		num n = new num();

		Thread ch = new Thread(new charec());
		ch.start();
		n.start();


		
	}

}
