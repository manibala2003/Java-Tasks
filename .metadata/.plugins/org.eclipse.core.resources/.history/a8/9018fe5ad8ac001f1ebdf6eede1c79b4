package file;
import java.io.*;
import java.util.*;

public class filehandling {
	public static void main(String args[]) {
		try {
			File f = new File("ex.txt");
			Scanner sc = new Scanner(f);
			while (sc.hasNextLine()) {
				System.out.println(sc.nextLine());
				
			}
		} catch (Exception e) {
			System.out.println(e);
		}
		
		
	}

}
